{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Yarp": "Debug"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Information",
        "Yarp": "Debug"
      }
    }
  },
  "Authentication": {
    "Keycloak": {
      "Authority": "http://localhost:8080/realms/microservices",
      "RequireHttpsMetadata": false
    }
  },
  "Services": {
    "Catalog": {
      "BaseUrl": "http://localhost:5100"
    },
    "Order": {
      "BaseUrl": "http://localhost:5200"
    }
  }
}
